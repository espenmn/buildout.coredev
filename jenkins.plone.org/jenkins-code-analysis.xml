  <builders>
    <hudson.tasks.Shell>
      <command>cp ../../Plone_4.2/lastSuccessful/archive/buildstamp $WORKSPACE
cp -r ../../Plone_4.2/workspace/packages $WORKSPACE
</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command>echo &quot;zptlint started...&quot;
find -L packages -regex &quot;.*\.[c|z]?pt&quot; | xargs -r $ZPTLINT | perl -p -e &apos;s/\s+$/ /g;s/\s+/ /g;s/\*{3}s?/\n/g&apos; &gt; zptlint.log
echo &quot;zptlint completed...&quot;</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command>echo &quot;pep8 started...&quot;
find -L packages -regex &quot;.*\.py&quot; | xargs -r pep8 | awk -F\: &apos;{printf &quot;%s:%s: [%s]%s\n&quot;, $1, $2, substr($4,2,4), substr($4,6)}&apos; &gt; pep8.log
echo &quot;pep8 completed...&quot;</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command>#!/bin/bash
echo &quot;pyflakes started...&quot;
find -L packages -regex &quot;.*\.py&quot;|xargs -r pyflakes &amp;&gt; pyflakes.log
echo &quot;pyflakes completed...&quot;</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command>echo &quot;ohcount started...&quot;
$OHCONVERT packages &gt; ohcount.log
echo &quot;ohcount completed...&quot;</command>
    </hudson.tasks.Shell>
